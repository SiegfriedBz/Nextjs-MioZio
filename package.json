{
  "name": "app_server_food_delivery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:cloudinary": "npx dotenv -e .env.local -- tsx ./seed/seedCloudinary.ts",
    "seed:db": "npx dotenv -e .env.local -- tsx ./seed/seedDB.ts",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "production:seedDB:build": "npx prisma generate && npx prisma migrate deploy && tsx ./seed/seedDB.ts && next build"
  },
  "engines": {
    "node": "18.17.1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^5.5.2",
    "cloudinary": "^1.41.0",
    "framer-motion": "^10.16.4",
    "next": "14.0.0",
    "next-auth": "^4.24.4",
    "react": "^18",
    "react-countdown": "^2.3.5",
    "react-dom": "^18",
    "react-toastify": "^9.1.3",
    "tailwind-merge": "^2.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.6",
    "autoprefixer": "^10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "tailwindcss": "^3",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
